	<manifest>
		<assemblies>
			<assembly name="Zongsoft.Scheduling.Cron" />
		</assemblies>

		<dependencies>
			<dependency name="Zongsoft.Scheduling" />
		</dependencies>
	</manifest>

	<extension path="/Workbench/Scheduling/Triggers">
